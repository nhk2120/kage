kage = function(x, y, z) {
if (is.numeric(x) | is.numeric(y) == TRUE) {
warning("Must put double quotes for argument x and y")
} else {
if (z == T) {
(as.numeric(substr(y, start = 1, stop = 4)) -
as.numeric(substr(x, start = 1, stop = 4))) + 1
} else if (z == F) {
MESS::age(x, y)
} else {
warning("Must put either T of F for argument")
}
}
}
use_package(MESS)
library(devtools)
use_package(MESS)
use_package(MESS)
check(kage)
